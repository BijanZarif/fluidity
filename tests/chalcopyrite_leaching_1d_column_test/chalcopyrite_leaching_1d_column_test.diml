m=2.0
p0=17.0  #923.13

for n in range(field.node_count):
  y=msh.node_val(n)
  rho_w=rho_wn.node_val(n)
  rho_a=rho_an.node_val(n)
  rgh=(rho_w-rho_a)*9.8*y[0]
