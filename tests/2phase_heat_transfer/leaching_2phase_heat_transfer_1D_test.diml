        <scalar_field name="Source" rank="0">
          <diagnostic>
            <mesh name="PressureMesh"/>
            <algorithm name="scalar_python_diagnostic" material_phase_support="multiple">
              <string_value lines="20" type="code" language="python">from math import sin,cos,pow,exp
prt=1.0-0.400488031
keff1=0.02
cp=4.185
rho=983.0
dF=states["Phase1"].scalar_fields["feox_dFe2_dt"]
msh=states["Phase1"].vector_fields["Coordinate"]
sat=states["Phase2"].scalar_fields["Saturation"]
vel=states["Phase2"].vector_fields["DarcyVelocity"]

prt1=0.400488031
D=5.5e-07
x0=400.0
t0=1000.0*2.0

for n in range(field.node_count):
  keff=keff1*prt/(cp*rho)
  x=msh.node_val(n)[0]
  ul=vel.node_val(n)[0]
  if (time&gt;1.0*dt):
    ul=vel.node_val(n)[0]
  else:
    ul=2.16647168421509E-006
  Ts=303.16-exp(x/x0)+cos(time/t0)
  Tl=333.16-exp((ul/D)*x/x0)+sin(time/t0)
  lq=sat.node_val(n)*prt1
  ud=ul/D
  dsrc=dF.node_val(n)*(-102.3)/(cp*rho)
  dlqT=lq*cos(time/t0)/t0
  ddut=lq*D*exp((x/x0)*ud)*pow(ud/x0,2.0)-ul*exp((x/x0)*ud)*ud/x0+dlqT
  ls=ddut-keff*(Ts-Tl)-dsrc

  field.set(n,ls)</string_value>
